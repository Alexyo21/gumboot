## currently not supported
#splashimage (sd0)/bootmii/splash.xpm.gz

#color red/blue green/black white/blue white/blue

#color blue/green green/black white/magenta white/magenta
timeout 1

default 0

title wii-linux-ngx\nOpen default boot menu
	## currently not supported
	#find --set-root /zImage
	root (sd0)/bootmii
	kernel /zImage
	boot

## 'write' is currently not supported
